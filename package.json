{
  "name": "yosys-rest",
  "version": "1.0.0",
  "description": "Run Yosys in GCR.",
  "keywords": [
    "typescript",
    "template"
  ],
  "author": {
    "name": "Hendrik Mennen",
    "url": "https://protop-solutions.com"
  },
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/chriswells0/node-typescript-template.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node -r dotenv/config ./dist/index",
    "dev": "ts-node-dev --no-deps --respawn --poll --interval 1000 -r dotenv/config src/server.ts"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/express": "^4.17.17",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.14.2",
    "chai": "^4.3.7",
    "conventional-changelog-cli": "^2.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.0.3",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "express": "^4.18.2",
    "sitka": "^1.0.6"
  }
}
